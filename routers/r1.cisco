! Site 1, Router 1
enable
config terminal
ip route 192.168.2.0 255.255.255.128 GigabitEthernet0/1
ip route 192.168.2.128 255.255.255.128 GigabitEthernet0/1
hostname r1
ipv6 unicast-routing
! To switch top
interface GigabitEthernet0/0
 no shutdown
exit
! Chef
interface GigabitEthernet0/0.20
 encapsulation dot1q 20
 ip address 192.168.1.1 255.255.255.128
 ipv6 enable
 ipv6 address 2000:0:0:10::1/64
 ipv6 ospf 1 area 0
exit
! Employee
interface GigabitEthernet0/0.30
 encapsulation dot1q 30
 ip address 192.168.1.129 255.255.255.128
 ipv6 enable
 ipv6 address 2000:0:0:11::1/64
 ipv6 ospf 1 area 0
exit
! To r4
interface GigabitEthernet0/1
 no shutdown
 ip address 10.0.70.1 255.255.255.252
 ipv6 enable
 ipv6 address fd00:70::1/64
 ipv6 ospf 1 area 0
exit
router ospf 1
 network 10.0.70.0 0.0.0.3 area 0
 network 192.168.1.0 0.0.0.127 area 0
 network 192.168.1.128 0.0.0.127 area 0
exit
ipv6 router ospf 1
 router-id 1.1.1.1
exit
exit
exit
!
