! WAN, Router 3
enable
config terminal
ip route 192.168.2.0 255.255.255.128 GigabitEthernet0/1
ip route 192.168.2.128 255.255.255.128 GigabitEthernet0/1
hostname r3
ipv6 unicast-routing
! To s9
interface GigabitEthernet0/0
 no shutdown
exit
! to r6
interface GigabitEthernet0/0.40
 encapsulation dot1q 40
 ip address 10.0.40.1 255.255.255.252
 ipv6 enable
 ipv6 address fd00:40::1/64
 ipv6 ospf 1 area 0
exit
! to r4
interface GigabitEthernet0/0.50
 encapsulation dot1q 50
 ip address 10.0.50.2 255.255.255.252
 ipv6 enable
 ipv6 address fd00:50::2/64
 ipv6 ospf 1 area 0
exit
! To r0
interface GigabitEthernet0/1
 no shutdown
 ip address 10.0.10.2 255.255.255.252
 ipv6 enable
 ipv6 address fd00:10::2/64
 ipv6 ospf 1 area 0
exit
! To r5
interface GigabitEthernet0/2
 no shutdown
 ip address 10.0.20.2 255.255.255.252
 ipv6 enable
 ipv6 address fd00:20::2/64
 ipv6 ospf 1 area 0
exit
router ospf 1
 network 10.0.10.0 0.0.0.3 area 0
 network 10.0.20.0 0.0.0.3 area 0
 network 10.0.40.0 0.0.0.3 area 0
 network 10.0.50.0 0.0.0.3 area 0
exit
ipv6 router ospf 1
 router-id 1.1.1.3
exit
exit
exit
!
